<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <add name="CtrlUpContext" connectionString="Data Source=(LocalDb)\MSSQLLocalDB;Initial Catalog=CtrlUpEmploye1;Integrated Security=SSPI;" providerName="System.Data.SqlClient" />
